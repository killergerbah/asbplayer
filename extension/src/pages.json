{
    "pages": [
        {
            "host": "www\\.netflix\\.com",
            "pageScript": "netflix-page.js",
            "syncAllowedAtPath": "watch",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "(m|www)\\.youtube\\.com",
            "pageScript": "youtube-page.js",
            "syncAllowedAtPath": "watch|shorts|embed",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "tver\\.jp",
            "pageScript": "tver-page.js",
            "syncAllowedAtPath": "episodes",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "www\\.b-ch\\.com",
            "pageScript": "bandai-channel-page.js",
            "syncAllowedAtPath": "titles",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "www\\.amazon\\..+|www\\.primevideo\\.com",
            "pageScript": "amazon-prime-page.js",
            "syncAllowedAtPath": ".*",
            "autoSync": {
                "enabled": true,
                "videoSrc": "^blob:.+"
            },
            "ignoreVideoElements": {
                "class": "tst-video-overlay-player-html5",
                "style": {
                    "display": "none"
                }
            }
        },
        {
            "host": "www\\.hulu\\..+",
            "pageScript": "hulu-page.js",
            "syncAllowedAtPath": "watch",
            "autoSync": {
                "enabled": true,
                "elementId": "content-video-player"
            }
        },
        {
            "host": "www\\.disneyplus\\..+",
            "pageScript": "disney-plus-page.js",
            "syncAllowedAtPath": "video|play",
            "searchShadowRootsForVideoElements": true,
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "www\\.apps\\.disneyplus\\..+",
            "pageScript": "apps-disney-plus-page.js",
            "syncAllowedAtPath": "watch",
            "searchShadowRootsForVideoElements": true,
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "video.unext.jp",
            "pageScript": "unext-page.js",
            "syncAllowedAtPath": "play",
            "autoSync": {
                "enabled": false
            }
        },
        {
            "host": ".+\\.viki\\.(io|com)",
            "pageScript": "viki-page.js",
            "syncAllowedAtPath": "videos",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "(jellyfin.*)|(emby.*)|(:8096$)",
            "pageScript": "emby-jellyfin-page.js",
            "syncAllowedAtHash": "#!/videoosd/videoosd.html|#/video",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "www.twitch.tv",
            "allowVideoElementsWithBlankSrc": true
        },
        {
            "host": "osnplus.com",
            "pageScript": "osnplus-page.js",
            "syncAllowedAtPath": "watch",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "www.bilibili.tv",
            "pageScript": "bilibili-page.js",
            "syncAllowedAtPath": "play|video",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "tv.nrk.no",
            "pageScript": "nrk-tv-page.js",
            "syncAllowedAtPath": ".*",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "(plex.*)|(:32400$)",
            "pageScript": "plex-page.js",
            "syncAllowedAtPath": ".*",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "areena.yle.fi",
            "pageScript": "areena-yle-page.js",
            "syncAllowedAtPath": ".*",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "play.max.com",
            "pageScript": "hbo-max-page.js",
            "syncAllowedAtPath": "watch",
            "autoSync": {
                "enabled": true
            }
        },
        {
            "host": "web.stremio.com",
            "pageScript": "stremio-page.js",
            "syncAllowedAtPath": ".*",
            "autoSync": {
                "enabled": true
            },
            "hideRememberTrackPreferenceToggle": true
        }
    ]
}
